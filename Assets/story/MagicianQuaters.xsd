<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="location" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element Name="name">
    <xs:complexType>
      <xs:attribute Name="value" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element Name="Npc">
    <xs:complexType>
      <xs:attribute Name="Name" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element Name="pre">
    <xs:complexType>
      <xs:sequence>
        <xs:element Name="At" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:attribute Name="location" type="xs:string" />
          </xs:complexType>
        </xs:element>
        <xs:element ref="Has" minOccurs="0" maxOccurs="unbounded" />
        <xs:element ref="global" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element Name="Has">
    <xs:complexType>
      <xs:attribute Name="value" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element Name="global">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Has" minOccurs="0" maxOccurs="unbounded" />
        <xs:element ref="Effect" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element Name="Effect">
    <xs:complexType>
      <xs:attribute Name="value" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element Name="Choices">
    <xs:complexType>
      <xs:sequence>
        <xs:element Name="choice" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:attribute Name="Name" type="xs:string" />
          </xs:complexType>
        </xs:element>
        <xs:element Name="repeatChoice" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="Name" minOccurs="0" maxOccurs="unbounded" />
              <xs:element ref="results" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element Name="onceChoice" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="Name" minOccurs="0" maxOccurs="unbounded" />
              <xs:element ref="results" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element Name="results">
    <xs:complexType>
      <xs:sequence>
        <xs:element Name="wingame" type="xs:string" minOccurs="0" />
        <xs:element Name="EffectResults" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="global" minOccurs="0" maxOccurs="unbounded" />
              <xs:element ref="Effect" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element Name="locationResults" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="Name" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element Name="ChoicesResults" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="Name" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element Name="location" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Name" />
        <xs:element ref="Npc" />
        <xs:element ref="pre" />
        <xs:element ref="Has" />
        <xs:element ref="global" />
        <xs:element ref="Effect" />
        <xs:element ref="Choices" />
        <xs:element ref="results" />
        <xs:element Name="Npcs">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="Npc" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element Name="Quests">
          <xs:complexType>
            <xs:sequence>
              <xs:element Name="RepeatableQuest" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element ref="Name" minOccurs="0" maxOccurs="unbounded" />
                    <xs:element ref="pre" minOccurs="0" maxOccurs="unbounded" />
                    <xs:element ref="Choices" minOccurs="0" maxOccurs="unbounded" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element Name="oneshotQuests" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element ref="Name" minOccurs="0" maxOccurs="unbounded" />
                    <xs:element ref="pre" minOccurs="0" maxOccurs="unbounded" />
                    <xs:element ref="Choices" minOccurs="0" maxOccurs="unbounded" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element Name="randomQuests" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element ref="Name" minOccurs="0" maxOccurs="unbounded" />
                    <xs:element Name="Npc" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element ref="Npc" minOccurs="0" maxOccurs="unbounded" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element ref="pre" minOccurs="0" maxOccurs="unbounded" />
                    <xs:element ref="Choices" minOccurs="0" maxOccurs="unbounded" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>